	 <reportRecord  id="demandeprix_report01" label="demande.de.prix" action_ref="teratech_achat_bes_1_1"  extern="false">
	      <template>
		     <![CDATA[
			   <div>
			      <ng-template src='base_account/views/styles.xml'></ng-template>
                  <div ng-repeat='obj in temporalDatas'>
				       <ng-template src='base_account/views/header.xml'></ng-template>
					   <label>N Compte</label><p>{{obj.fournisseur.code}}</p>
					   <label>Libelle</label><p>{{obj.fournisseur.label}}</p>
					   <table class="table  table-striped table-bordered table-hover table-condensed">
						   <thead> 
							  <tr style="font-weight: bold;">
								 <td>Article</td><td>Quantité</td><td>PU HT</td>
							  </tr>
						   </thead>
						   <tbody>
							  <tr ng-repeat='obj2 in obj.lignes'>
								 <td>{{obj2.article.designation}}</td>
								 <td>{{obj2.quantite}}</td>
								 <td>{{obj2.puht}}</td>
							  </tr>
						   </tbody>
					   </table>					   
					  <!--ADD_PAGE-->
					  <footer>Pied de page</footer>
				   </div>
				 </div>	  
			   ]]>
		  </template>
	 </reportRecord>
	 <reportRecord  id="reponse_report01" label="deemande.de.prix" action_ref="teratech_achat_bes_1_1"  extern="false">
	      <search entity="ReponseToCommande" module="teratechachat" method="command">
		  </search>
		  <template>
		     <![CDATA[
			    <div>
                    <ng-template src='base_account/views/styles.xml'></ng-template>				
					<ng-template src='base_account/views/header.xml'></ng-template> 				
					<div ng-repeat='obj in temporalDatas'>   
					   <label>N Compte</label><p>{{obj.fournisseur.code}}</p>
					   <label>Libelle</label><p>{{obj.fournisseur.label}}</p>
					   <table class="table  table-striped table-bordered table-hover table-condensed">
						   <thead> 
							  <tr>
								 <td>Article</td><td>Quantité</td><td>PU HT</td><td>Remise</td><td>Sous total(HT)</td>
							  </tr>
						   </thead>
						   <tbody>
							  <tr ng-repeat='ligne in obj.lignes'>
								 <td>{{ligne.article.designation}}</td>
								 <td>{{ligne.quantite}}</td>
								 <td>{{ligne.puht}}</td>
								 <td>{{ligne.remise}}</td>
								 <td>{{ligne.totalht}}</td>
							  </tr>
							  <tr><td></td><td></td><td></td><td>Total HT</td><td>{{obj.totalht}}</td></tr>
							  <tr><td></td><td></td><td></td><td>Taxes</td><td>{{obj.taxes}}</td></tr>
							  <tr><td></td><td></td><td></td><td>Total TTC</td><td>{{obj.totalttc}}</td></tr>
						   </tbody>
					   </table>					   
				   </div>							   
				 </div>	  
			   ]]>
		  </template>
	 </reportRecord>
