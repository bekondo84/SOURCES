<?xml version="1.0" encoding="UTF-8"?>
<keren>
    <theme>
	     <principal>
		       <template>
			          <![CDATA[
						<div  style="height: 100%;width:100%;">					  
							  <!-- Navigation -->					    
							 <style>
								 .principal-background {
									  background: url(http://localhost:8080/keren/auth/resource/static/material.jpg) ;
									  background-repeat: initial;
								 }
								 .table-striped > tbody > tr:nth-child(odd) > td, .table-striped > tbody > tr:nth-child(odd) > th {
									   background-color: white;
									}
								 .table-striped > tbody > tr:nth-child(even) > td, .table-striped > tbody > tr:nth-child(even) > th {
									   background-color: #efefef;
									}
								 .modal-header{
									  padding:15px;
									  border-bottom:1px solid #e5e5e5;
									  background-color:#f5f5f5;
								   }
								 .modal-footer{
									 padding:15px;
									 border-top:1px solid #e5e5e5;
									 background-color:#f5f5f5;
									 text-align:right;
								  }
								 .navbar-inverse{
									background-color
								 }
							 </style>
							 <ng-template src="theme/views/principaltemplate.xml"></ng-template>
					   </div>
                      ]]>		
			    </template>			 
		 </principal>	
         <containertemplate>		       
		       <template>
			      <![CDATA[	
					   <div class="col-sm-12  col-md-12" id="container"  style="height: 100%;width: 100%;padding-right: 0px;padding-left: 0px;">   
					   </div>
				   ]]>	
			   </template>
		 </containertemplate>	
         <treetemplate>
		   <template>
		     <![CDATA[
                <div  style="height: 100%;width:100%;">			 
					  <style type="text/css">
					      .table-striped > tbody > tr:nth-child(odd) > td, .table-striped > tbody > tr:nth-child(odd) > th {
									   background-color: white;
									}
						  .table-striped > tbody > tr:nth-child(even) > td, .table-striped > tbody > tr:nth-child(even) > th {
							   background-color: #efefef;
							}
						  .search__field {
							background-color: transparent;
							border: solid 1px #f5f5f5;
							width: 100%;
							border-bottom: solid 1px #dddddd;
							border-radius: 0;
							cursor: pointer; 
						   }
						.search__icon {
						  position: absolute;
						  top: 0;
						  right: 0;
						  background-color: #e9f1f4;
						  width: 10px;
						  height: 20px;
						  font-size: 1.35em;
						  text-align: center;
						  border-color: transparent;
						  border-radius: 50%;
						  pointer-events: none;
						  display: inline-block; 
						}
						.panel-container{
							background: #9d9d9d ;
						}
						.calendar-panel{
							 background-color: white;
							 margin: 20px;
							 padding: 5px;
						}
						.dashboard-header{
							 background-color: #f5f5f5 !important;
							 color:#897575 !important;
							 border-color:#f5f5f5 !important;						 
						}
						.dashboard-body{
						   
						}

						</style>
						<ng-template src="theme/views/treetemplate.xml"></ng-template>
				 </div>
			 ]]>
		   </template>
         </treetemplate>
         <formtemplate>
		     <template>
			     <![CDATA[ 
				   <div style="height: 100%;width:100%;">
						 <style type="text/css">
							.panel-container{
								background: #9d9d9d ;
							}
							.workflowbar{
								margin-top: -10px;
								margin-bottom: 0px;
								border-top:solid 1px #a8a8a8;
								background-color: #f0eeee;
								background-image: linear-gradient(to bottom, #fcfcfc, #dedede);
							}
						</style>
						<ng-template src="theme/views/formtemplate.xml"></ng-template>
					</div>				  
				 ]]>
			 </template>
		 </formtemplate>		 
	</theme>
</keren>