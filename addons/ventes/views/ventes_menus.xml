	 <reportRecord  id="devisvte_report01" label="Demande de prix" action_ref="teratech_vente_ope_1"  extern="false">
	      <template>
		     <![CDATA[
			    <div>				   
					<ng-template src='base_account/views/header.xml'></ng-template> 				
					<div ng-repeat='obj in temporalDatas'>   
					   <label>N Compte</label><p>{{obj.fournisseur.code}}</p>
					   <label>Libelle</label><p>{{obj.fournisseur.label}}</p>
					   <table class="table  table-striped table-bordered table-hover table-condensed">
						   <thead> 
							  <tr style="font-weight: bold;">
								 <td>Article</td><td>Quantité</td><td>PU HT</td><td>Remise</td><td>Sous total(HT)</td>
							  </tr>
						   </thead>
						   <tbody>
							  <tr ng-repeat='ligne in obj.lignes'>
								 <td>{{ligne.article.designation}}</td>
								 <td>{{ligne.quantite}}</td>
								 <td>{{ligne.puht}}</td>
								 <td>{{ligne.remise}}</td>
								 <td>{{ligne.totalht}}</td>
							  </tr>
							  <tr><td></td><td></td><td></td><td>Total HT</td><td>{{obj.totalht}}</td></tr>
							  <tr><td></td><td></td><td></td><td>Taxes</td><td>{{obj.taxes}}</td></tr>
							  <tr><td></td><td></td><td></td><td>Total TTC</td><td>{{obj.totalttc}}</td></tr>
						   </tbody>
					   </table>					   
				   </div>							   
				 </div>	   
			   ]]>
		  </template>
	 </reportRecord>
