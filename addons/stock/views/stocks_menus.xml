	 <kabanentry id="article_kaban_01" parent_ref="teratech_stock_str_1">
	    <template>
		    <![CDATA[				   
			 <div class='col-md-12 col-sm-12' id='detail-panel-body-header'> 
				 <div class='col-md-3 col-sm-4'>
				  <div id='imageContent'  style='float: left;margin-left: -20px;'> <img id='apercuImageContent' src='{{protocol}}://{{hostname}}:{{portvalue}}/keren/auth/resource/static/{{item.image}}' alt='Image ' ng-click='imageClick()' height='70' width='75'/>
				  </div> 
				 </div>
				 <div  class='col-md-8 col-sm-7'>
				  <div style='font-weight: bold;'>{{item.code | cut:true:20:'...'}}
				  </div>
				  <div>{{item.intitule | cut:true:50:'...'}}
				  </div> 
				  <div> 
					<div class='col-sm-6 col-md-6 pull-left' >{{item.reference}}
					</div>	
				  </div> 
				 </div> 
			  </div>
		  ]]>		
